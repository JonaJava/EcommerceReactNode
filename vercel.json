{
    "builds":[
        {
            "src":"index.js",
            "use":"@vercel/node"
        },
        {
            "src":"build/**",
            "use":"@vercel/static"
        }
    ],
    "routes":[
        {
            "src":"/products",
            "dest":"app.js"
        },
        {
            "src":"/products/(.*)",
            "dest":"app.js"
        },
        {
            "src":"/categories",
            "dest":"app.js"
        },
        {
            "src":"/categories/(.*)",
            "dest":"app.js"
        },
        {
            "src":"/brands",
            "dest":"app.js"
        },
        {
            "src":"/brands/(.*)",
            "dest":"app.js"
        },
        {
            "src":"/users",
            "dest":"app.js"
        },
        {
            "src":"/users/(.*)",
            "dest":"app.js"
        },
        {
            "src":"/auth",
            "dest":"app.js"
        },
        {
            "src":"/auth/(.*)",
            "dest":"app.js"
        },
        {
            "src":"/cart",
            "dest":"app.js"
        },
        {
            "src":"/cart/(.*)",
            "dest":"app.js"
        }

    ]

}
{ "rewrites": [{ "source": "/(.*)", "destination": "/" }] }